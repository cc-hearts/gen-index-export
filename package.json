{
  "name": "@cc-heart/gen-index-export",
  "version": "0.3.0",
  "description": "Generate the exported statement",
  "main": "index.js",
  "bin": "dist/bin/index.js",
  "types": "dist/types/src/define.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/define.esm.js",
      "types": "./dist/types/src/define.d.ts"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "npm run prebuild && tsc -p tsconfig.build.json && rollup -c",
    "playground": "ts-node-esm index.ts --path ./example/a ./example/b --output ./example/a/index.ts ./example/b/test.ts --recursive --ignoreIndexPath",
    "deploy": "npm run prebuild && npm run build",
    "prettier": "npx prettier --write ."
  },
  "keywords": [
    "export"
  ],
  "author": "heart",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.22.4",
    "@babel/traverse": "^7.22.4",
    "@cc-heart/utils": "^2.0.12",
    "@cc-heart/utils-service": "^0.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "commander": "^11.0.0",
    "glob": "^9.3.5",
    "ora": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/babel__traverse": "^7.20.1",
    "@types/node": "^18.16.2",
    "rimraf": "^4.4.1",
    "tslib": "^2.5.0",
    "vitest": "^0.29.8"
  }
}
